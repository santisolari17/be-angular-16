{
  "name": "072025-angular-16",
  "version": "1.0.3",
  "engines": {
    "npm": "9.x.x",
    "node": "18.x.x"
  },
  "scripts": {
    "preinstall": "node check-node-version.js",
    "ng": "ng",
    "analyze": "webpack-bundle-analyzer dist/072025-angular-16/stats.json",
    "build": "ng build --configuration production --base-href=/apps/072025-angular-16/",
    "build:stats": "ng build --base-href=/apps/072025-angular-16/ --stats-json",
    "coverage": "jest --coverage",
    "compodoc": "npx compodoc -p tsconfig.doc.json -s --theme postmark",
    "lint": "ng lint",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "format": "prettier --write \"./src/**/*.{ts,json,html}\"",
    "lint-staged": "npx lint-staged -r",
    "lint-format": "npm run format && ng lint",
    "start": "node server.js",
    "stats": "webpack-bundle-analyzer dist/*/stats.json",
    "test": "jest --coverage --verbose --reporter=lcov --reporter=text --forceExit --runInBand",
    "version": "npx conventional-changelog -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepare": "husky install",
    "server:stubby": "nodemon --config stubs/nodemon-stubs.json",
    "dev:local": "ng serve --configuration local",
    "dev:stubby": "concurrently \"npm run server:stubby\" \"npm run dev:local\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.6",
    "@angular/common": "16.2.6",
    "@angular/compiler": "16.2.6",
    "@angular/core": "16.2.6",
    "@angular/forms": "16.2.6",
    "@angular/localize": "16.2.6",
    "@angular/platform-browser": "16.2.6",
    "@angular/platform-browser-dynamic": "16.2.6",
    "@angular/router": "16.2.6",
    "asd": "file:./local_modules/asd-1.16.4.tgz",
    "beche-utils-lib": "file:./local_modules/beche-utils-lib-1.1.3.tgz",
    "class-validator": "0.14.1",
    "dayjs": "1.11.10",
    "helmet": "7.0.0",
    "jsencrypt": "3.3.2",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "method-override": "3.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "uuid": "9.0.1",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "16.0.1",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.6",
    "@angular/compiler-cli": "16.2.6",
    "@briebug/jest-schematic": "6.0.0",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@compodoc/compodoc": "1.1.22",
    "@faker-js/faker": "^9.9.0",
    "@types/jest": "29.5.7",
    "@types/lodash": "^4.17.20",
    "@types/node": "20.8.10",
    "@types/stubby": "4.1.6",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "concurrently": "8.2.2",
    "conventional-changelog-cli": "3.0.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "13.1.2",
    "ng-mocks": "14.11.0",
    "nodemon": "3.1.10",
    "prettier": "3.0.3",
    "stubby": "5.1.0",
    "ts-node": "10.9.1",
